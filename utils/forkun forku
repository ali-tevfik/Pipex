#include <stdio.h>
#include <unistd.h>
#include <sys/wait.h>
int main()
{
	int id = fork();
	int id1 = fork();

	id =  0 && id1= 0 => cocugun cocugu
	id = 0  && id1 != 0 => cocugun parente
	id != 0 && id1 != 0 => parent
	id != 0 && id1 == 0 => parnten cocugu
	if (id == 0)
	{
		if (id1 == 0)
		{
			printf("y current %d parent %d\n",getpid(),getppid());
		}
		else
		{
			printf("x currnt %d parrent %d\n",getpid(),getppid());
		}
	}
	else
	{
		if (id1 == 0)
		{
			printf("z currnt %d parrent %d\n",getpid(),getppid());
		}
		else
			printf("oke currnt %d parrent %d\n",getpid(),getppid());
	}
	//wait(NULL);
	return 0;
}
